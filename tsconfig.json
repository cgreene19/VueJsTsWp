        "allowSyntheticDefaultImports": true,        
        "outDir": "./built/",
        "sourceMap": true,
        "strict": true,
        "noImplicitReturns": true,
        "module": "es2015",
        "moduleResolution": "node",
        "target": "es5",
        "noImplicitAny": false
    },
    "exclude": [
        "node_modules"
    ]
}